{
  "name": "dead-trend-tracker-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.6.0",
    "web3": "^4.3.0",
    "firebase-admin": "^12.0.0",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "@pinata/sdk": "^2.1.0",
    "@google/generative-ai": "^0.1.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "nodemon": "^3.0.2",
    "hardhat": "^2.19.2",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.1",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "fast-check": "^3.15.0",
    "ts-jest": "^29.1.1"
  },
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "compile:contracts": "npx hardhat compile",
    "test:contracts": "npx hardhat test",
    "deploy:mumbai": "npx hardhat run scripts/deploy.ts --network mumbai",
    "deploy:polygon": "npx hardhat run scripts/deploy.ts --network polygon"
  }
